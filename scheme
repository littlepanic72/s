3.历史表（日表）
create table his
（
	date
	code
	name
	close
	high
	low
	open
	yesclose
	updownvalue
	updown
	volume
	turnover
）

4.历史表2 跟历史表1一致

5.最高最低表 hl
create table hl
(
	date
	code
	name
	days
	h
	l
)

6.值得关注表
create table gz
(
	date
	code
	name
	price
)

7.当天临时表
create table today
(
	code 
	name
	price
	updown
	pe
	msfum 每份收益
	mfradio2 净利润
	mfradio10 主营收
	tcap 总市值
	mcap 流通市值
	turnmover 成交额
	vulume 成交量
	lb	量比
	hs	换手率
	five 五分钟涨跌幅
	high	最高值
	low	最低值
	open	开市
	yesclose	昨天收盘价
)

8.当天临时表2
create table today
(
	date 日期
	times 第几次
	code 
	name
	price
	updown
	pe
	msfum 每份收益
	mfradio2 净利润
	mfradio10 主营收
	tcap 总市值
	mcap 流通市值
	turnmover 成交额
	vulume 成交量
	lb	量比
	hs	换手率
	five 五分钟涨跌幅
	high	最高值
	low	最低值
	open	开市
	yesclose	昨天收盘价
)

